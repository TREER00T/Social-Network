{
  "name": "social-network-server",
  "version": "1.1.0",
  "description": "Social Network Application",
  "main": "init.js",
  "scripts": {
    "start": "nodemon init.js",
    "unit-test": "jest"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/app/"
    ],
    "testMatch": [
      "**/test/**/*.test.js"
    ]
  },
  "author": "Ali Azmoodeh Valadi",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "node-jose": "^2.1.1",
    "openrtp": "^1.0.5",
    "opensql": "^1.2.47",
    "socket.io": "^4.5.3",
    "swagger-ui-express": "^4.5.0",
    "wfl": "^1.0.4",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "commander": "^9.4.1",
    "dotenv": "^16.0.3",
    "jest": "^29.3.1",
    "multer": "^1.4.4",
    "nodemon": "^2.0.20"
  },
  "wfl": {
    "size": "15m",
    "where": {
      "error": "18m",
      "info": "18m",
      "warning": "18m"
    },
    "path": "./Logger",
    "offset": "1d",
    "accessToDelete": false
  }
}
